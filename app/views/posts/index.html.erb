<%= render 'shared/navbar' %>
<div class='all-content'>
  <% @posts.each do |post| %>
    <div class='post'>
      <h1><%= post.user.username %></h1>
      <div class='inner-post'>
        <img src='https://cdn.mos.cms.futurecdn.net/aKN5fXTyQagKGscKuSDaf9-1200-80.jpg'>
        <div class='inner-post-bottom'>
          <p>post.description</p>
          <div class='post-info'>
            <p><%= post.created_at %></p>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
