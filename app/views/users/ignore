    <% images = [] %>
      <% @user.photos.each do |photo| %>
        <% images.push(cl_image_tag photo.key) %>
        <%= images %>
      <% end %>
      <div class="back-to-index">
        <%= link_to users_path(@user) do %>
          <i class="fa fa-arrow-left index"></i>
        <% end %>
      </div>
    </div>
